<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
    <option name="COLLAPSE_FILE_HEADER" value="false" />
  </component>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="LSPCodeLens" value="Default" />
        <entry key="component.usage" value="Default" />
        <entry key="inheritors" value="Default" />
        <entry key="references" value="Default" />
        <entry key="rename" value="Default" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
    <option name="defaultPosition" value="Top" />
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="component.usage" />
        <option value="inheritors" />
        <option value="references" />
        <option value="vcs.code.vision" />
      </set>
    </option>
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="0" />
  </component>
  <component name="DeclarativeInlayHintsSettings">
    <option name="enabledOptions">
      <map>
        <entry key="js.type.hints#function.returns" value="false" />
        <entry key="js.type.hints#non.paren.single.param" value="true" />
        <entry key="js.type.hints#parameters.in.parens" value="true" />
        <entry key="js.type.hints#variables.and.fields" value="true" />
        <entry key="ts.type.hints#function.returns" value="false" />
        <entry key="ts.type.hints#non.paren.single.param" value="true" />
        <entry key="ts.type.hints#parameters.in.parens" value="true" />
        <entry key="ts.type.hints#variables.and.fields" value="true" />
      </map>
    </option>
    <option name="providerIdToEnabled">
      <map>
        <entry key="js.chain.hints" value="true" />
        <entry key="js.type.hints" value="true" />
        <entry key="ts.chain.hints" value="true" />
        <entry key="ts.enum.hints" value="true" />
        <entry key="ts.type.hints" value="true" />
      </map>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="REMOVE_TRAILING_BLANK_LINES" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="false" />
    <option name="IS_CARET_BLINKING" value="false" />
    <option name="SHOW_STICKY_LINES" value="false" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
    <option name="IS_FULL_LINE_HEIGHT_CURSOR" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="PHP.parameter.hints.old" />
  </component>
  <component name="MarkdownCodeFoldingSettings">
    <option name="collapseFrontMatter" value="false" />
    <option name="collapseLinks" value="false" />
    <option name="collapseTableOfContents" value="false" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="ffffff" />
      <option name="BACKGROUND" value="5ea1ff" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="ffffff" />
      <option name="BACKGROUND" value="ffbd5e" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bhack\b.*">
      <option name="FOREGROUND" value="ffffff" />
      <option name="BACKGROUND" value="ff5ea0" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="tagTreeHighlightingEnabled" value="false" />
  </component>
  <component name="XmlFoldingSettings">
    <option name="COLLAPSE_HTML_STYLE_ATTRIBUTE" value="false" />
    <option name="COLLAPSE_ENTITIES" value="false" />
    <option name="COLLAPSE_DATA_URI" value="false" />
  </component>
</application>